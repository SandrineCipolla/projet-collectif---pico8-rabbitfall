--carottes functions

function create_carottes()
	carottes={}
	for i=1,20 do
		local new_carotte={
			x=rnd(128),
			y=rnd(128),
			col=15,
			speed=4
		}
		add(carottes,new_carotte)
	end
end

function update_carottes()
	for c in all(carottes) do
		c.y+=c.speed
		if c.y>128 then
			c.y=0
			c.x=rnd(128)
		end
	end
end

function draw_carottes()
	for c in all(carottes) do
	 spr(66,c.x,c.y)
	end
end

