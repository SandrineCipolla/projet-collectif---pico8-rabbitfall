--draw functions

function _draw_game()
cls()
map()
print("score: "..score,cx+2,cy+2,8)
print("lives: "..p.life,cx+2,cy+10,8)
camera(cx,cy)
draw_piece()
end

function draw_gameover()
cls(3)
music(-1)
print("game over",cx+40,cy+40,7)
end

function draw_menu()
	cls(1)
	print("bravo!",cx+62,cy+30,7)
	print("bienvenue au niveau suivant!",cx+15,cy+37,7)
	print("press â to continue",cx+30,cy+55,2)
	print("press â to continue",cx+31,cy+56,8)
	spr(128,cx+10,cy+10,2,2)
	if game_state>=1 and game_state<4 then
		if (btn(â)) then
			_draw=_draw_game 
			cx+=128
			restart()
		end
	elseif game_state==4 then
		if (btn(â)) then
			_draw=_draw_game
			cx+=128
			kill_rabbit()
		end
	end
end


function draw_menugame()
cls()
draw_carottes()
--		cls()

		cprint("rabbitfall",20,9)
		cprint("rabbitfall",21,4)
		cprint("press â to continue",110,3)
		cprint("press â to continue",111,11)
	
	--menu
	for i=1,#sel do
		col=7
			if cur_sel==i then col=10 end			cprint(sel[i],50+i*8,col)
	end
end


	-- centrer le texte
	function cprint(s,y,col)
		print(s,64-#s*2,y,col)
	end

