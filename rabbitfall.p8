function _init()
cx=0
cy=0
dx=0
dy=0
g=0.001
score=0
game_state=0

sel={"play","exit"}
cur_sel=1

music(0)
_draw=draw_menugame
_update=update_menugame
create_carottes()
create_pieces()
end




