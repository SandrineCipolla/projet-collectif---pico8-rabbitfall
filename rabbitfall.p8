--update functions

function hit(x,y,w,h)
	collide=false
	
	for i=x,x+w,w do
		if fget(mget(i/8,y/8),1) or
					fget(mget(i/8,(y+h)/8),1) then
					collide=true			
		end
	end		
	
	return collide		
end

function victory(x,y,w,h)
	bottommap=false
	
	for i=x,x+w,w do
		if fget(mget(i/8,y/8),4) or
					fget(mget(i/8,(y+h)/8),4) then
					bottommap=true			
		end
	end
		return bottommap
end

function restart()
	p.x=cx+rnd(100)+10
	p.y=cy+rnd(100)+10
end


function teleport(x,y,w,h)
	tp=false
	
	for i=x,x+w,w do
		if fget(mget(i/8,y/8),6) or
					fget(mget(i/8,(y+h)/8),6) then
					tp=true			
		end
	end
	
	return tp

end

function gravity_fall(x,y,w,h)
	fall=false
	
	for i=x,x+w,w do
		if fget(mget(i/8,y/8),2) or
					fget(mget(i/8,(y+h)/8),2) then
					fall=true			
		end
	end		
	
	return fall		
end

function bounce(x,y,w,h)
	bouncy=false
	
	for i=x,x+w,w do
		if fget(mget(i/8,y/8),7) or
					fget(mget(i/8,(y+h)/8),7) then
					bouncy=true			
		end
	end		
	
	return bouncy		
end

function _update_game()

	dy+=g
	if (btn(â¡ï¸)) dx=1
	if (btn(â¬…ï¸)) dx=-1
	if (btn(â¬‡ï¸)) dy=1
	
	p.x+=dx
	p.y+=dy
	
	if hit(p.x+dx,p.y,7,7) then
		dx=0
		p.life-=1
		if p.life==0 then
			sfx(0)
			p.x=1024
			_draw=draw_gameover
		end
	end
	if hit(p.x,p.y+dy,7,7) then
		dy=0
		p.life-=1
		if p.life==0 then
			sfx(0)
			p.x=1024
			_draw=draw_gameover
		end
	end
		
	if victory(p.x+dx,p.y,7,7) then
		dx=0
		dy=0
		g=0.001
		game_state+=1
			if game_state>=1 then
			sfx(1)
			kill_rabbit()
			_draw=draw_menu
			score+=500		
			end
	end
	if victory(p.x,p.y+dy,7,7) then
		dx=0
		dy=0
		g=0.001
		game_state+=1
		if game_state>=1 then
			sfx(1)
			kill_rabbit()
			_draw=draw_menu
			score+=500		
		end
	end
	
	if gravity_fall(p.x+dx,p.y,7,7) then
		g-=0.01
	end
	if gravity_fall(p.x,p.y+dy,7,7) then
		g-=0.01
	end
	
	if bounce(p.x+dx,p.y,7,7) then
		dx-=2.5
	end
	if bounce(p.x,p.y+dy,7,7) then
		dy-=2.5
	end
	
	if teleport(p.x+dx,p.y,7,7) then
		p.x=30
	end
	if teleport(p.x,p.y+dy,7,7) then
		p.y=30
	end
	
	if update_score(p.x+dx,p.y,7,7) then
		score+=100
	end
	if update_score(p.x,p.y+dy,7,7) then
		score+=100
	end
	
end


function update_score(x,y,w,h)
	bonus=false
	
	for i=x,x+w,w do
		if fget(mget(i/8,y/8),5) or
					fget(mget(i/8,(y+h)/8),5) then
					bonus=true			
		end
	end		
	
	return bonus		
end

function update_menugame()
	update_carottes()
	if btnp(â¬†ï¸) then cur_sel-=1 end
	if btnp(â¬‡ï¸) then cur_sel+=1 end
	cur_sel=mid(1,cur_sel,#sel)
	
	if btnp(â) then
		if cur_sel==1 then -- game
			_draw=_draw_game
			_update=_update_game
		elseif cur_sel==2 then --exit
			cls()
			cprint("merci d avoir joue a notre jeu",cx+55,8)
			cprint("a bientot ğŸ˜",cx+65,2)
			
			stop()
		end
	end	
end

function kill_rabbit()
	p.x=1024
	p.y=0
end
